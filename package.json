{
  "name": "nuxtjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
    "generate": "nuxt-ts generate",
    "format": "prettier --write \"**/*.{js,jsx,json,ts,tsx,md,mdx,css,html,yml,yaml,scss,sass}\" --ignore-path .gitignore",
    "lint": "eslint --ext vue,ts --ignore-path .gitignore .",
    "lint:fix": "eslint --ext vue,ts --ignore-path .gitignore . --fix",
    "test": "jest",
    "test-cover": "jest --coverage",
    "test:w": "npm run test-cover -- --watch"
  },
  "dependencies": {
    "@nekohack/normalize.css": "^2.1.1",
    "@nuxt/http": "^0.6.2",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/pwa": "^3.3.5",
    "nuxt": "^2.14.6",
    "nuxt-svg-loader": "^1.2.0",
    "vue-google-charts": "^0.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@nekohack/eslint-config-vue": "^1.0.0",
    "@nuxt/types": "^2.14.6",
    "@nuxt/typescript-build": "^2.0.3",
    "@nuxt/typescript-runtime": "^2.0.0",
    "@types/jest": "^26.0.21",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "@vue/test-utils": "^1.1.0",
    "babel-core": "7.0.0-bridge.0",
    "core-js": "^3.6.5",
    "eslint": "^7.10.0",
    "fibers": "^5.0.0",
    "jest": "^26.5.0",
    "prettier": "^2.3.2",
    "sass": "^1.32.8",
    "sass-loader": "^10.1.1",
    "ts-jest": "^26.4.1",
    "vue-jest": "^3.0.4"
  }
}
